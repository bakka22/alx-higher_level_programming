#!/usr/bin/node
let arr = [3, 3, 5];
console.log(arr.length);
const argv = require('node:process').argv;
console.log(argv[0]);
console.log(argv[1]);
console.log(argv[2]);
if (argv.length === 1) {
  console.log('No arguments');
}
else if (argv.length === 2) {
  console.log('Argument found');
}
else {
  console.log('Arguments found');
}
